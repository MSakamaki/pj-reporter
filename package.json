{
  "name": "pj-reporter",
  "version": "0.0.2",
  "description": "protractor + puppeteer + jasmine reporter",
  "main": "dist/index.js",
  "scripts": {
    "wdm.update":"./node_modules/protractor/bin/webdriver-manager update --gecko=false",
    "checkformat": "prettier src/**/*.ts --list-different",
    "format": "prettier src/**/*.ts --write",
    "e2e": "del-cli pdf/* && npm run build && protractor example/conf.js && ls -la pdf/*",
    "test": "npm run build && jest dist",
    "build": "npm run clean && tsc -p tsconfig.json",
    "clean": "del-cli dist",
    "test.all": "npm run checkformat && npm run test && npm run e2e"
  },
  "keywords": [
    "protractor",
    "puppeteer",
    "jasmine"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@types/fs-extra": "^5.0.2",
    "cheerio": "^1.0.0-rc.2",
    "fs-extra": "^5.0.0"
  },
  "engines": {
    "node": ">=8.9",
    "npm": "~5.6.0node"
  },
  "peerDependencies": {
    "puppeteer": "^1.2.0",
    "jasmine-core": "~2.8.0"
  },
  "devDependencies": {
    "@types/cheerio": "^0.22.7",
    "@types/jasmine": "^2.8.6",
    "@types/jest": "^22.2.3",
    "@types/node": "^9.6.4",
    "@types/puppeteer": "^1.2.1",
    "del-cli": "^1.1.0",
    "jasmine-spec-reporter": "^4.2.1",
    "jest": "^22.4.3",
    "prettier": "^1.12.1",
    "protractor": "^5.3.1",
    "puppeteer": "^1.2.0",
    "ts-node": "^5.0.1",
    "tslint-config-prettier": "^1.10.0",
    "tslint-plugin-prettier": "^1.3.0",
    "typescript": "^2.8.1"
  }
}
